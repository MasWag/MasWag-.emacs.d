prefix=/usr
bindir=$prefix/bin
libdir=$prefix/lib
datadir=$prefix/share
mandir=$prefix/share/man
sysconfdir=/etc
localstatedir=$prefix/var
libruby=/usr/lib/ruby
librubyver=/usr/lib/ruby/2.0.0
librubyverarch=/usr/lib/ruby/2.0.0/x86_64-linux
siteruby=/usr/lib/ruby/site_ruby
